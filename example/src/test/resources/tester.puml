@startconfig
vin = "987"
eventid = "123"
@endconfig

@startconfig
vin = "120"
eventid = "20"
@endconfig

SEQUENCE @startuml

participant CCCMock
participant XCS
participant Tester
participant SVDS

CCCMock -> SVDS : GET "svds/vehicle/internal/v2/${vin}"
activate SVDS
SVDS --> CCCMock : 200 - (marketRegion : "1")
deactivate SVDS

Tester -> XCS : GET "xcs/vin/v1/${vin}"
activate XCS
XCS --> Tester : 200
deactivate XCS

Tester -> CCCMock : GET "event/${eventid}"
activate CCCMock
CCCMock --> Tester : 200 - (eventData : "1")
deactivate CCCMock

XCS -> SVDS : GET "test"
activate XCS
SVDS --> XCS : 200
deactivate XCS

@enduml
